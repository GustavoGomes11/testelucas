function solucao(horaChegada) {
	//seu codigo aqui
let hora = horaChegada - 1;
    console.log(hora);
}

function processData(input) {
	const horaChegada = Number(input);
    solucao(horaChegada);
} 

process.stdin.resume();
process.stdin.setEncoding("ascii");
_input = "";
process.stdin.on("data", function (input) {
    _input += input;
});

process.stdin.on("end", function () {
   processData(_input);
});
